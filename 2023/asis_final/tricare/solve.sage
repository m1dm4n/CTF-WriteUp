
for bits in range(1200, 3000):
    t = (
        4.
    )
    at = arctan(t)
    pin = pi.n(bits)
    L = matrix(QQ, [[1, 0, 0], [at, 1, at], [pin, 0, pin]])
    L[:, 0] *= 2**bits
    L = L.LLL()
    L[:, 0] /= 2**bits

    m = abs(round(L[1][-1])) // 9
    flag = int(m).to_bytes((m.bit_length() + 7) // 8, "big")
    if b"ASIS" in flag:
        break
print(flag)
